<% if(carousel.includes('first')){ %>
<script>
$(document).ready(function(){
    $('.carousel').slick({
    infinite: true,
    slidesToShow: 9,
    slidesToScroll: 1,
    });
});
</script>
<% } %>

<style scoped>
    .carousel img{
        height: 80px;
    }

/* slick but */

    .slick-prev,
    .slick-next {
        display: block;
        position: absolute;
        font-size: 0;
        -webkit-text-stroke-width: 2px;
        top:0;
        width:3rem;
        height: 100%;
        border: 0;
        /* color: rgb(var(--rgb-6)); */
        -webkit-text-stroke-color: rgb(var(--rgb-6));
        background-color: hsl(var(--hsl-1),5%,30%);
        z-index: 1;
        transition: all 0.5s ease;
    }

    .slick-prev {
        left: 0px;
    }

    .slick-next {
        right: 0px;
        /* text-align: right; */
    }

    .slick-prev:after {
        content: "<";
        font-size: 2rem;
    }

    .slick-next:after {
        content: ">";
        font-size: 2rem;
    }

    .slick-prev:hover,
    .slick-next:hover {
        background-color: rgb(var(--rgb-1));
    }

    .slick-prev:hover:after,
    .slick-next:hover:after {
        /* color: rgb(var(--rgb-6)); */
        font-size: 2.5rem;
        -webkit-text-stroke-color: rgb(var(--rgb-3));
        text-shadow: 1px 1px 5px rgb(var(--rgb-6)), 1px 1px 5px rgb(var(--rgb-6)), 1px 1px 10px rgb(var(--rgb-3));
    }

</style>

